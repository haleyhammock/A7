<html>
	<head>
		<title>Wellness Forum</title>
		<meta charset="utf-8">
		<script type="text/javascript" src="https://code.jquery.com/jquery-1.7.1.min.js"></script>
		<script src="js/bootstrap.js"></script>

		<meta name="viewport" content="width=device-width, initial-scale=1.0">

		<link href="/css/bootstrap.min.css" rel="stylesheet">
		<link href="/css/bootstrap-theme.min.css" rel="stylesheet">
		<link href="/css/bootstrap.css" rel="stylesheet">
		<link href="/css/bootstrap-4.6.css" rel="stylesheet">
		<link href="/css/bootstrap.min-4.6.css" rel="stylesheet">



		<script src="js/bootstrap.js"></script>
		<script src="js/bootstrap-4.6.js"></script>

		<script src="/js/bar.js"></script>
		<script src="routes/addRecords.js"></script>
	</head>

	<body>
		<nav class="navbar navbar-expand-md bg-dark navbar-dark">
		<a class="navbar-brand" href="home"> Wellness Forum</a>
		<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar">
    		<span class="navbar-toggler-icon"></span>
  		</button>
		<div id="navbar" class="collapse navbar-collapse">
			<ul class="nav navbar-nav">
				<li class="nav-item">
					<a class="nav-link" href="home">Home</a>
				</li>
				<li class="nav-item">
					<a class="nav-link" href="record">Record</a>
				</li>
				<li class="nav-item">
					<a class="nav-link" href="forum">Forum</a>
				</li>
				<li class="nav-item active">
					<a class="nav-link" href="profile">Profile</a>
				</li>
				<li class="nav-item">
					<a class="nav-link" href="help">Help</a>
				</li>
				<li>
					<button type="button" id="logout" class="btn btn-danger">Log Out</button>
				</li>
			</ul>
		</div>
	</nav>
	<div id="main" class="jumbotron">
		{{#each data}}
	    <div>
	    	<img class="profile-pic" id='profpic'src="images/default_profile_picture.JPG" width = "200" height = "200"alt="Profile Picture">
			<h3 class="name" id='name'>{{name}}</h3>

			<p class="age">Age: {{age}}</p>
			<p class="location"> Location: {{city}}, {{state}}</p>
		</div>
		{{/each}}
	</div>

	<h4>Recent Records</h4>
	{{#each records}}
	<button class="buttonPost">
	<div class="recent">
		<p class="record_date">Date: {{date}}</p>
		<p class="record_subject">Subject: {{subject}}</p>
		<p class="record_title">Title: {{title}}</p>
		<p class="record_input">Notes: {{input}}</p>
		<!-- <p class="record_hours">{{question}}: {{input}}</p> -->
	</div>
    </div>
	{{/each}}


	</body>
</html>
